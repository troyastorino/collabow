exports.locals = { 
  title : 'collabow'
  ,description: 'Your Page Description'
  ,author: 'Your Name'
  ,analyticssiteid: 'XXXXXXX' 
};

exports.error = function(res, msg, err) {
  res.json({
    result: "error",
    msg: msg,
    error: err
  });
};

exports.success = function(res) {
  res.json({
    result: "success"
  });
};
